{
    "configurations": [
        {
            "name": "WSL",
            "includePath": [
                "${workspaceFolder}/*",
                "${workspaceFolder}/ac-library/**",
                "${workspaceFolder}/include/**",
                "${workspaceFolder}/library/**",
                "${workspaceFolder}/tpl/**"
            ],
            "defines": [
                "DEBUG",
                "ATCODER"
            ],
            "compilerPath": "/usr/bin/g++-12",
            "cStandard": "c11",
            "cppStandard": "c++20",
            "intelliSenseMode": "linux-gcc-x64"
        },
        {
            "name": "Mac",
            "includePath": [
                "${workspaceFolder}/*",
                "${workspaceFolder}/ac-library/**",
                "${workspaceFolder}/include/**",
                "${workspaceFolder}/library/**",
                "${workspaceFolder}/tpl/**",
                "/opt/homebrew/include"
            ],
            "defines": [
                "DEBUG",
                "ATCODER"
            ],
            "compilerPath": "/opt/homebrew/bin/g++-12",
            "cStandard": "c11",
            "cppStandard": "c++20",
            "intelliSenseMode": "macos-gcc-arm64"
        }
    ],
    "version": 4
}